{
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "build",
    "target": "es2017",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "fifo-cache/src/*",
    "lfu-cache/src/*",
    "lru-cache/src/*",
    "rr-cache/src/*",
    "slru-cache/src/*",
    "ttl-cache-v1/src/*",
    "ttl-cache-v2/src/*",
    "spec.ts"
  ]
}
